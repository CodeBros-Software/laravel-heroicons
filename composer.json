{
    "name": "codebros-nl/laravel-heroicons",
    "description": "A Laravel Package to include heroicons as blade components in any project",
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CodeBros\\LaravelHeroicons\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Norman Reinhard",
            "email": "info@codebros.nl"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeBros\\LaravelHeroicons\\Providers\\LaravelHeroiconsServiceProvider"
            ]
        }
    }
}
